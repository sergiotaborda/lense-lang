

import lense.core.collections.Sequence;
import helloworld.start.types.AdditiveMonoid;

public enhancement SumEnhancement extends Sequence<T>  given T is AdditiveMonoid<T>
{

	public sum() : T {
		let type = typeOf(T);
		mutable let total = type.zero(); // the type of T is an instance of AdditiveMonoid<T>
		for (let item in this){
			total = total + item; 
		}
		return total;
	}

}  