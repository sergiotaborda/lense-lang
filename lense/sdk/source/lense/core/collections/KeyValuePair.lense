

import lense.core.math.Integer;
import lense.core.lang.Any;
import lense.core.lang.Boolean;
import lense.core.lang.String;
import lense.core.lang.HashValue;

public class KeyValuePair<K,V> {

	public constructor ( val public key : K, val public value: V);

	
	public hashValue() : HashValue {
		return key.hashValue().concat(value.hashValue()); // TODO ++ operator
	}
	
	public equalsTo(other : Any) : Boolean {
		return other is KeyValuePair && other.key == this.key && other.value == this.value;
	}
	
	public asString() : String {
		return  this.key.asString() + " " + this.value.asString();
	}
}