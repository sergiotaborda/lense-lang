package teste;

class Interpolation{
	
public lense.lang.Void  main(){
final lense.lang.String name =  new lense.lang.String ("Robert");
final lense.lang.Real pi =  new lense.lang.Decimal ( new lense.lang.String ("3.1415"));
final lense.lang.String s =  new lense.lang.String ("the value of \u03C0 is ") + pi.toString() +  new lense.lang.String (".");
final lense.lang.Boolean t = false;
final lense.lang.String p =  new lense.lang.String ("It's not ") + t.toString() +  new lense.lang.String (" my name id ") + name;
}
}
